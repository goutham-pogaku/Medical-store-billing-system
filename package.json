{
    "name": "medical-store-billing",
    "version": "1.0.0",
    "description": "Medical Store Billing System with Stock Management",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "install-client": "cd client && npm install"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.3.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.0",
        "uuid": "^9.0.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "concurrently": "^8.2.0",
        "nodemon": "^3.0.1"
    }
}
